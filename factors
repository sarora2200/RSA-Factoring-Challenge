#!/usr/bin/env python3

import sys

def factorize(n):
    factors = []
    for i in range(2, n // 2 + 1):
        if n % i == 0:
            factors.append((i, n // i))
            break
    return factors

def main():
    if len(sys.argv) != 2:
        print("Usage: factors <file>")
        return

    file_name = sys.argv[1]

    with open(file_name, "r") as file:
        lines = file.readlines()

    for line in lines:
        number = int(line.strip())

        # Skip 1 since it cannot be factored into two smaller numbers
        if number == 1:
            continue

        factorization = factorize(number)

        if factorization:
            factor1, factor2 = factorization[0]
            print(f"{number}={factor1}*{factor2}")

if __name__ == "__main__":
    main()
