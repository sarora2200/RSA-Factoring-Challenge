#!/usr/bin/python3

import math
import sys
import time

def factorize(n):
    factors = []
    for i in range(2, int(math.sqrt(n)) + 1):
        while n % i == 0:
            factors.append(i)
            n //= i
    if n > 1:
        factors.append(n)
    return factors

def factorize_file(file_path):
    with open(file_path, 'r') as file:
        for line in file:
            n = int(line.strip())
            factors = factorize(n)
            p = factors[0]
            q = n // p
            print(f'{n}={p}*{q}')

if __name__ == '__main__':
    import sys
    if len(sys.argv) != 2:
        print("Usage: python factors.py <file>")
        sys.exit(1)
    file_path = sys.argv[1]
    factorize_file(file_path)
